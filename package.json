{
  "name": "flight-school",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "style": "prettier . --write",
    "typeCheck": "tsc --noEmit",
    "check": "npm run style && npm run lint && npm run typeCheck",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "storybook build --quiet && start-server-and-test 'http-server storybook-static --port 6006 --silent' http://localhost:6006 'npm run test-storybook'",
    "test-playwright": "playwright test",
    "test-playwright:ui": "playwright test --ui",
    "test-playwright:ci": "start-server-and-test dev http://localhost:3000 'npm run test-playwright'",
    "unit-test": "jest",
    "test": "npm run unit-test && npm run test-storybook && npm run test-playwright"
  },
  "dependencies": {
    "@mantine/core": "^7.11.2",
    "@mantine/hooks": "^7.11.2",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.44.4",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.46.1",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.6",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.2.9",
    "@storybook/nextjs": "^8.2.6",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/test-runner": "^0.19.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.10.1",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.40",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.3.3",
    "start-server-and-test": "^2.0.5",
    "storybook": "^8.2.6",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.4"
  }
}
